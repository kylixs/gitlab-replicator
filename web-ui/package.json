{
  "name": "web-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test -c ../tests/playwright.e2e.config.ts",
    "test:e2e:ui": "playwright test -c ../tests/playwright.e2e.config.ts --ui",
    "test:e2e:debug": "playwright test -c ../tests/playwright.e2e.config.ts --debug",
    "test:integration": "playwright test -c ../tests/playwright.integration.config.ts",
    "test:integration:ui": "playwright test -c ../tests/playwright.integration.config.ts --ui",
    "test:integration:debug": "playwright test -c ../tests/playwright.integration.config.ts --debug"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@playwright/test": "^1.57.0",
    "axios": "^1.13.2",
    "crypto-js": "^4.2.0",
    "echarts": "^6.0.0",
    "element-plus": "^2.13.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.24",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^4.0.16",
    "@vue/tsconfig": "^0.8.1",
    "happy-dom": "^20.0.11",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.1.4"
  }
}
